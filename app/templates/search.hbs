    <div class="container-fluid osf-light-background">
        <nav id="create" class="navbar navbar-default navbar-background navbar-fixed-top">
            <div class="container-fluid" id="browse">
                <form class="navbar-form navbar-left" role="search">
                    <img class="navbar-brand" src="https://osf.io/static/img/cos-white2.png">
                    <p class="navbar-brand">Search</p>
                    <div class="form-group">
                        {{input value=searchQuery placeholder="Search..." class="navbar-form form-control" action="search" on="key-up"}}
                    </div>
                    <button type="submit" class="btn btn-default" {{action "search"}}>Submit</button>
                    <button class="btn btn-outline-primary" {{action "forward"}}>More Results</button>
                    <h5 class="navbar-text">Page: {{page}}</h5>
                </form>
            </div>
        </nav>
          <div class="row" id="indexBottom">
              <div class="col-md-12 table-view">
                  <div class="col-md-10 col-md-offset-1 fade-background">
                      <div class="col-md-12">
                          <div class="row">
                                <div class="col-md-4">
                                    Title
                                </div>
                                <div class="col-md-4">
                                    Location
                                </div>
                                <div class="col-md-4"> 
                                  Description
                                </div>
                          </div>
                          {{#each model as |conference|}}
                            {{#link-to "conference.index.index" conference.id tagName="div" class="row"}}
                                <div class="col-md-4">
                                    {{conference.title}}
                                </div>
                                <div class="col-md-4">
                                    {{conference.city}}, {{conference.state}}
                                </div>
                                <div class="col-md-4"> 
                                  {{truncate conference.description limit=200}}
                                </div>
                            {{/link-to}}
                          {{/each}}
                      </div>
                  </div> <!-- end conference-view-inner-container -->
              </div> <!-- end table-view -->
          </div><!-- end conference-view-outer-container-->
      {{outlet}}
    </div> <!-- end container fluid -->

{{yield}}