    <div class="container-fluid osf-light-background">
        <nav id="create" class="navbar navbar-default navbar-background navbar-fixed-top">
            <div class="container-fluid" id="browse">
                <form class="navbar-form navbar-left" role="search">
                    <img class="navbar-brand" src="https://osf.io/static/img/cos-white2.png">
                    <p class="navbar-brand">Search</p>
                    <div class="form-group">
                        {{input value=searchQuery placeholder="Search..." class="navbar-form form-control" id='searchQuery' action="search"}}
                    </div>
                    <button type="submit" class="btn btn-default" {{action "search"}}>Search</button>
                    <button class="btn btn-outline-default" {{action "forward"}}>More Results</button>
                </form>
                <h5 class="navbar-text">
                Page: <strong><em>{{model.meta.pagination.page}}</em></strong> of {{model.meta.pagination.pages}}
                </h5>
            </div>
        </nav>
        {{#if this.onePage}}
        {{else}}
        <div class="row ">
              <div class="col-md-4 col-md-offset-4">
                <div class="pagination">
                  <div class="btn-toolbar" role="toolbar">
                    <button class="btn btn-success"{{action "toPage" 1}}> First</button> 
                    <div class="btn-group" role="group" aria-label="Toolbar with button groups">
                      {{#each this.buttonArray as |page|}}  
                        <button class="btn btn-default"{{action "toPage" page}}> {{page}}</button>
                      {{/each}}                   
                    </div>
                    <button class="btn btn-danger"{{action "toPage" model.meta.pagination.pages}}> Last</button>
                  </div>
                </div>
              </div>
          </div>
          {{/if}}
          <div class="row" id="indexBottom">
              <div class="col-md-12 table-view">
                  <div class="col-md-7 col-md-offset-2 fade-background">
                  {{#if this.results}}
                      <div class="col-md-12">
                          <div class="row table-head">
                                <div class="col-md-3">
                                    <h4>Title</h4>
                                </div>
                                <div class="col-md-3">
                                    <h4>Location</h4>
                                </div>
                                <div class="col-md-6"> 
                                  <h4>Description</h4>
                                </div>
                          </div>
                          {{#each model as |conference|}}
                            {{#link-to "conference.index.index" conference.id tagName="div" class="row"}}
                                <div class="col-md-3">
                                    <h5><strong><em>{{conference.title}}</em></strong></h5>
                                </div>
                                <div class="col-md-3">
                                    <h6>{{conference.city}}, {{conference.state}}</h6>
                                </div>
                                <div class="col-md-6"> 
                                  <h6>{{truncate conference.description limit=200}}</h6>
                                </div>
                            {{/link-to}}
                          {{/each}}
                      </div>
                      {{else}}
                      <h4> There are no results for <em><strong>{{this.query}}</strong></em></h4>
                      {{/if}}
                  </div> <!-- end conference-view-inner-container -->
              </div> <!-- end table-view -->
            </div>
            {{#if this.onePage}}
            {{else}}
            <div class="row ">
              <div class="col-md-4 col-md-offset-4">
                <div class="pagination">
                  <div class="btn-toolbar" role="toolbar">
                    <button class="btn btn-success"{{action "toPage" 1}}> First</button>
                    <div class="btn-group" role="group" aria-label="Toolbar with button groups">
                      {{#each this.buttonArray as |page|}}  
                        <button class="btn btn-default"{{action "toPage" page}}> {{page}}</button>
                      {{/each}}                   
                    </div>
                    <button class="btn btn-danger"{{action "toPage" model.meta.pagination.pages}}> Last</button>
                  </div>
                </div>
              </div>
          </div>
          {{/if}}
      {{outlet}}
    </div> <!-- end container fluid -->

{{yield}}